{
  "name": "auth0-account-link-extension",
  "version": "2.7.0",
  "description": "Auth0 Account Link Extension",
  "main": "index.js",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "node ./index.js",
    "test": "nyc --all mocha test --recursive --timeout 5000",
    "test:integration": "mocha integration --recursive --timeout 50000",
    "lint": "eslint .",
    "serve:dev": "gulp run",
    "client:build": "minify --clean --output dist/assets/link.$npm_package_version.min.css public/css/link.css && minify --clean --output dist/assets/admin.$npm_package_version.min.css public/css/admin.css",
    "extension:build": "a0-ext build:server ./webtask.js ./dist && cp ./dist/auth0-account-link.extension.$npm_package_version.js ./build/bundle.js",
    "build": "yarn run client:build && yarn run extension:build"
  },
  "author": "Auth0",
  "license": "MIT",
  "auth0-extension": {
    "bundleModules": true,
    "nodeTarget": "12",
    "externals": [
      "auth0-extension-hapi-tools@1.3.1",
      "auth0-extension-tools@1.5.0",
      "auth0@2.28.0",
      "boom@3.2.2",
      "hapi-auth-jwt2@7.0.1",
      "@auth0/hapi@13.6.0",
      "inert@4.0.1",
      "joi@9.0.4",
      "jsonwebtoken@8.5.1",
      "jwks-rsa@1.9.0",
      "lodash@3.10.1",
      "nconf@0.10.0",
      "open@0.0.5",
      "request@2.88.2",
      "webtask-tools",
      "winston@1.0.0"
    ]
  },
  "dependencies": {
    "@auth0/hapi": "13.6.0",
    "auth0": "^2.28.0",
    "auth0-extension-hapi-tools": "1.3.1",
    "auth0-extension-tools": "1.5.0",
    "boom": "3.2.2",
    "hapi-auth-jwt2": "7.0.1",
    "inert": "4.0.1",
    "joi": "9.0.4",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "1.9.0",
    "lodash": "^3.10.1",
    "nconf": "^0.10.0",
    "open": "^0.0.5",
    "request": "^2.88.2",
    "webtask-tools": "^3.4.1",
    "winston": "1.0.0"
  },
  "devDependencies": {
    "@babel/runtime": "7.11.2",
    "auth0-extensions-cli": "^1.1.2",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.8.1",
    "eslint-config-auth0": "^11.1.0",
    "eslint-config-auth0-base": "^13.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "gulp": "^3.9.1",
    "gulp-nodemon": "^2.5.0",
    "gulp-util": "^3.0.8",
    "minifier": "^0.8.1",
    "mocha": "^3.5.0",
    "ngrok": "^2.2.15",
    "nodemon": "^1.11.0",
    "nyc": "^11.1.0",
    "puppeteer": "^0.11.0"
  }
}
