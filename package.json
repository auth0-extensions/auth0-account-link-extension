{
  "name": "auth0-account-link-extension",
  "version": "3.1.0",
  "description": "Auth0 Account Link Extension",
  "main": "index.js",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "start": "node ./index.js",
    "test": "nyc --all mocha test --recursive --timeout 5000",
    "test:integration": "mocha integration --recursive --timeout 50000",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "serve:dev": "gulp run",
    "client:build": "minify --clean --output dist/assets/link.$npm_package_version.min.css public/css/link.css && minify --clean --output dist/assets/admin.$npm_package_version.min.css public/css/admin.css",
    "extension:build": "a0-ext build:server ./webtask.js ./dist && cp ./dist/auth0-account-link-extension.extension.$npm_package_version.js ./build/bundle.js",
    "build": "yarn run client:build && yarn run extension:build"
  },
  "author": "Auth0",
  "license": "MIT",
  "auth0-extension": {
    "nodeTarget": "4.2.0",
    "bundleModules": true,
    "externals": [
      "auth0-extension-tools@1.5.2",
      "auth0@4.9.0",
      "bluebird@3.5.0",
      "body-parser@1.12.4",
      "boom@3.2.2",
      "compression@1.4.4",
      "debug@2.2.0",
      "delegates@0.1.0",
      "depd@1.0.1",
      "destroy@1.0.3",
      "ejs@2.3.1",
      "express-jwt@3.1.0",
      "express@4.12.4",
      "hapi-auth-jwt2@10.5.1",
      "@hapi/hapi@21.3.3",
      "hoek@4.1.0",
      "iconv-lite@0.4.10",
      "@hapi/inert@7.1.0",
      "jade@1.10.0",
      "@hapi/joi@17.1.1",
      "jsonwebtoken@9.0.0",
      "jwks-rsa@3.0.1",
      "lodash@4.17.21",
      "lru-cache@2.6.4",
      "lru-memoizer@1.10.0",
      "mime-db@1.10.0",
      "mime-types@2.0.12",
      "moment@2.10.3",
      "morgan@1.5.3",
      "ms@0.7.1",
      "nconf@0.12.0",
      "node-uuid@1.4.3",
      "open@0.0.5",
      "qs@3.1.0",
      "raw-body@2.1.0",
      "read-all-stream@2.1.2",
      "request@2.88.2",
      "superagent@1.2.0",
      "type-check@0.3.1",
      "webtask-tools@3.4.1",
      "winston@3.10.0",
      "xml2js@0.4.8",
      "@hapi/boom@10.0.1",
      "superagent@8.0.9"
    ]
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "@hapi/hapi": "21.3.3",
    "@hapi/inert": "^7.1.0",
    "@hapi/joi": "^17.1.1",
    "@ngrok/ngrok": "^1.4.1",
    "auth0": "4.9.0",
    "auth0-extension-tools": "^1.5.2",
    "hapi-auth-jwt2": "10.5.1",
    "jsonwebtoken": "9.0.0",
    "jwks-rsa": "3.0.1",
    "lodash": "4.17.21",
    "nconf": "0.12.0",
    "open": "^0.0.5",
    "request": "2.88.2",
    "superagent": "8.0.9",
    "webtask-tools": "^3.4.1",
    "winston": "3.10.0"
  },
  "devDependencies": {
    "auth0-extensions-cli": "^4.0.4",
    "chai": "^4.1.0",
    "eslint": "^4.3.0",
    "eslint-config-auth0": "^11.0.0",
    "eslint-config-auth0-base": "^13.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "gulp": "4.0.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-util": "^3.0.8",
    "minifier": "^0.8.1",
    "mocha": "^3.5.0",
    "nock": "^14.0.3",
    "nodemon": "^1.11.0",
    "nyc": "^11.1.0",
    "puppeteer": "^24.2.0",
    "sinon": "^19.0.2"
  },
  "resolutions": {
    "jsonwebtoken": "9.0.0"
  }
}
